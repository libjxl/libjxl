prefix=@CMAKE_INSTALL_PREFIX@
exec_prefix=${prefix}
libdir=@PKGCONFIG_TARGET_LIBS@
includedir=@PKGCONFIG_TARGET_INCLUDES@

Name: libjxl_gain_map
Description: Gain map support library for libjxl
Version: @JPEGXL_LIBRARY_VERSION@
@JPEGXL_REQUIRES_TYPE@: @JPEGXL_GAIN_MAP_LIBRARY_REQUIRES@
Libs: -L${libdir} -ljxl_gain_map @JPEGXL_GAIN_MAP_PUBLIC_LIBS@
Libs.private: @JPEGXL_GAIN_MAP_PRIVATE_LIBS@
Cflags: -I${includedir}
Cflags.private: -DJXL_GAIN_MAP_STATIC_DEFINE
